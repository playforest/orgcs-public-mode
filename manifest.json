{
    "manifest_version":3,
    "name":"OrgCS Public",
    "version":"1.0",
    "description":"Automatically sets all interactions in orgcs to public",

    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://orgcs.lightning.force.com/*"
            ]
        }
    ],
    "host_permissions":[
        "https://orgcs.lightning.force.com/*"
    ]
}